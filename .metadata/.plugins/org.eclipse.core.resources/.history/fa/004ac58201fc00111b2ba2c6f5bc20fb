package saveTheLolipop.utilitaire;


public enum EnumType {
	GUERRIER	(23,38,1),// "./images/guerrier.png"), 
	ARCHER		(23,38,2),//"./images/archer.png"),
	MAGE		(23,38,3);//"./images/mage.png");
	//ARBRE		(12,12,4))"./images/arbre.png");
	
	private int largeur;
	private int hauteur;
	private int code;
	
	private EnumType(int l, int h, int c) {
		largeur = l;
		hauteur = h;
		code = c;
	}
	
	public int largeur() {
		return largeur;
	}
	public int hauteur() {
		return hauteur;
	}
	public int code() {
		return code;
	}	
}
